{
  "name": "openapi-typescript-generator",
  "version": "0.1.0",
  "description": "Generate typescript with ajv validation based on openapi schemas",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --build --watch",
    "prepublish": "rm -rf dist && npm run build"
  },
  "author": "Q42",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Q42/openapi-typescript-generator.git"
  },
  "homepage": "https://github.com/Q42/openapi-typescript-generator",
  "dependencies": {
    "@openapi-contrib/openapi-schema-to-json-schema": "^3.0.4",
    "js-yaml": "^4.0.0",
    "json-schema-to-typescript": "^10.1.3",
    "object-hash": "^2.1.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.0",
    "@types/object-hash": "^1.3.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2"
  }
}
